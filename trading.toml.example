# Define instruments with their strategy-timeframe combinations
[[instruments]]
symbol = "NDX"
name = "NASDAQ 100"
enabled = true

# Data sources for this instrument (multiple sources for fallback)
data_sources = ["yfinance"]

# Strategy-timeframe mappings
[[instruments.strategies]]
name = "dummy_strategy_1"
timeframes = ["1D"]
enabled = true

# Parameters for this strategy-timeframe combination
[[instruments.strategies.parameters.1D]]
send_signal = true

[[instruments]]
symbol = "^GSPC"
name = "SP500"
enabled = true

# Data sources for this instrument (multiple sources for fallback)
data_sources = ["yfinance"]

# Strategy-timeframe mappings
[[instruments.strategies]]
name = "dummy_strategy_1"
timeframes = ["4H", "1D"]
enabled = true

# Parameters for 1D timeframe
[[instruments.strategies.parameters.1D]]
send_signal = true

# Parameters for 4H timeframe
[[instruments.strategies.parameters.4H]]
send_signal = false

[[instruments.strategies]]
name = "dummy_strategy_2"
timeframes = ["1D"]
enabled = true

# Parameters for dummy_strategy_2
[[instruments.strategies.parameters.1D]]
send_signal = true
